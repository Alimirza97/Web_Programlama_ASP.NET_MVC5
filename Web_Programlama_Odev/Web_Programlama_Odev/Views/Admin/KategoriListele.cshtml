@model IEnumerable<Web_Programlama_Odev.Models.Kategori>

@{
    ViewBag.Title = "KategoriListele";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Kategori Listele</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Kategori_Adi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Kategori_Adi)
        </td>
    </tr>
}

</table>
